<div class="content">
  <div class="container-fluid">

      <div class="row">
          <div class="col-lg-12 col-md-12">
            <div class="card">
              <div class="header">
                  <h4 class="title">ปรับยอดรวมรับรายวัน (Adjust)</h4>
                  <p class="category"></p>
              </div>
              <div class="content">

              </div>
              <div class="content table-responsive table-full-width">
                  <table class="table table-striped">
                      <thead>
                          <tr>
                              <th>วันที่</th>
                              <th>ศูนย์ตั้งรับ</th>                            
                              <th>จำนวน</th>
                              <th>เงินสด</th>
                              <th>รอโอน</th>
                              <th>รวม</th>
                              <th>ปรับยอด</th>
                              <th></th>
                          </tr>
                      </thead>
                      <tbody>
                          <% data.forEach(function(data) { %>
                            <tr>
                                <td><%= data.date %></td>
                                <td><%= data.service_point_group%></td>
                                <td><%= data.qty %></td>
                                <td><%= data.cash %></td>
                                <td><%= data.wait_transfer %></td>
                                <td><%= (data.cash + data.wait_transfer) %></td>
                                <td><%= data.adjust %></td>
                                <td><a href="/accounting/adjust/edit?id=<%= data.id %>" class="btn btn-default">แก้ไข</a></td>
                            </tr>
                          <% }); %>
                      </tbody>
                  </table>

                  <div class="content right">
                      <ul class="pagination">
                          <li><a href="#"><<</a></li>
                          <li><a href="#"><</a></li>
                          <li><a href="#">1</a></li>
                          <li><a href="#">2</a></li>
                          <li><a href="#">3</a></li>
                          <li><a href="#">4</a></li>
                          <li><a href="#">5</a></li>
                          <li><a href="#">></a></li>
                          <li><a href="#">>></a></li>
                      </ul>
                  </div>
              </div>
          </div>  
          </div>
      </div>
  </div>
</div>